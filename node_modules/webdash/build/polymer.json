{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/web-dash.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*",
    "index.html",
    "src/my-app.html",
    "src/web-dash.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "service-worker.js",
    "utils/*.js",
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "manifest.json"
  ],
  "builds": [
    {
      "name": "default",
      "bundle": false,
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addServiceWorker": true
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}